{
  "name": "@habx/eslint-config-client",
  "version": "6.4.0",
  "description": "Habx eslint configuration for clients.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/eslint-config-client.git"
  },
  "keywords": [
    "eslint",
    "habx",
    "lint"
  ],
  "author": "habx",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/eslint-config-client/issues"
  },
  "homepage": "https://github.com/habx/eslint-config-client#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "prettier-eslint": "^13.0.0"
  },
  "devDependencies": {
    "husky": "^5.2.0",
    "lint-staged": "^12.1.2"
  },
  "publishConfig": "https://npm.pkg.github.com/"
}
