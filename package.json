{
  "name": "@habx/eslint-config-client",
  "version": "6.1.0",
  "description": "Habx eslint configuration for clients.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/eslint-config-client.git"
  },
  "keywords": [
    "eslint",
    "habx",
    "lint"
  ],
  "author": "habx",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/eslint-config-client/issues"
  },
  "homepage": "https://github.com/habx/eslint-config-client#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "prettier-eslint": "^12.0.0"
  },
  "devDependencies": {
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4"
  },
  "publishConfig": "https://npm.pkg.github.com/"
}
