{
  "name": "@habx/eslint-config-client",
  "version": "6.11.0",
  "description": "Habx eslint configuration for clients.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/eslint-config-client.git"
  },
  "keywords": [
    "eslint",
    "habx",
    "lint"
  ],
  "author": "habx",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/eslint-config-client/issues"
  },
  "homepage": "https://github.com/habx/eslint-config-client#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.3.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "prettier-eslint": "^14.1.0"
  },
  "devDependencies": {
    "husky": "^5.2.0",
    "lint-staged": "^13.0.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
